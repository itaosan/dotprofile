{
  "cleanupPeriodDays": 60,
  "permissions": {
    "allow": [
      "Bash(npm:*)",
      "Bash(git:*)",
      "Bash(gh:*)",
      "Bash(cargo:*)",
      "Bash(curl:*)",
      "Bash(jq:*)",
      "Read(**)",
      "Edit(~/**)",
      "WebFetch(domain:*)",
      "Grep(**)",
      "Glob(**)",
      "LS(**)"
    ],
    "deny": [
      "Bash(rm -rf /)",
      "Bash(sudo rm:*)",
      "Bash(format:*)",
      "Read(node_modules/**)",
      "Read(.git/**)",
      "Read(*.log)",
      "Read(dist/**)",
      "Read(build/**)"
    ]
  },
  "model": "opus"
}